    "\"\"\"\n",
    "检查文件编码类型\n",
    "\"\"\"\n",
    "import chardet\n",
    " \n",
    " \n",
    "# 获取文件编码类型\n",
    "def get_encoding(file):\n",
    "    # 二进制方式读取，获取字节数据，检测类型\n",
    "    with open(file, 'rb') as f:\n",
    "        data = f.read()\n",
    "        return chardet.detect(data)['encoding']\n",
    " \n",
    "file_name = './id.json'\n",
    "encoding = get_encoding(file_name)\n",
    "print(encoding)"
   ]
  }
