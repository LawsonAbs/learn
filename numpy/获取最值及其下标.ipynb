    "import numpy as np\n",
    "a = np.array([1,2,3,3,4,5,0,0,5,2])\n",
    "# 得到最值\n",
    "max_value = np.amax(a)\n",
    "print(max_value)\n",
    "\n",
    "# 得到最值对应的下标，返回值是个tuple\n",
    "idx=np.where(a==max_value)\n",
    "print(idx)"
   ]
