    "'''unique()方法的使用\n",
    "'''\n",
    "import numpy as np\n",
    "arr = np.random.randint(1,10,5)\n",
    "print(arr)\n",
    "\n",
    "res = np.unique(arr)  # 默认情况下，会从小到大的对arr 数组进行排序，并去掉重复，返回一个数组\n",
    "print(res)\n",
    "print(type(res)) # <class 'numpy.ndarray'>"
   ]
  }
