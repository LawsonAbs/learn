    "\"\"\"\n",
    "找出最大值，以及最大值所在的下标\n",
    "\"\"\"\n",
    "import torch as t\n",
    "a = t.tensor([ -2.8309, -10.0151,  -0.0609, -10.9483])\n",
    "print(a)\n",
    "val = t.max(a) # 找出最大数\n",
    "print(val)\n",
    "idx = t.max(a,dim=0) # 找出最大值所在的下标\n",
    "print(idx) "
   ]
