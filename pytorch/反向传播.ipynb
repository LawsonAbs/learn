   "source": [
    "import torch as t\n",
    "x = t.tensor(3.5,requires_grad = True)\n",
    "print(x)\n",
    "y = (x-1)*(x-2)*(x-3)\n",
    "print(y) \n",
    "# 此时对x求导，则有 dy/dx = 3x^2 - 11x + 11\n",
    "y.backward() # 表示对 y开始做反向传播。\n",
    "print(x.grad) # 张量x 里梯度的数值"
   ]
  }
