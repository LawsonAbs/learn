   ],
   "source": [
    "\"\"\"topk\n",
    "topk既返回最大值，也会返回最大值所在的下标\n",
    "1.在一维中使用topk\n",
    "\"\"\"\n",
    "import torch as t\n",
    "x = t.arange(1,5)\n",
    "print(x)\n",
    "y = t.topk(x,3) # 返回的是一个元组\n",
    "print(y)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "tensor([[ 0.4298,  1.7276,  0.1461],\n",
      "        [ 0.0974,  1.6675, -0.4002]])\n"
     ]
    }
   ],
   "source": [
    "\"\"\"\n",
    "2.在二维tensor中使用topk\n",
    "\"\"\"\n",
    "a = t.randn(2,3)\n",
    "print(a)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "torch.return_types.topk(\n",
      "values=tensor([[1.7276, 0.4298],\n",
      "        [1.6675, 0.0974]]),\n",
      "indices=tensor([[1, 0],\n",
      "        [1, 0]]))\n",
      "tensor([[1, 0],\n",
      "        [1, 0]])\n"
     ]
    }
   ],
   "source": [
    "res = t.topk(a,k=2,dim=1)\n",
    "index = res[1]\n",
    "print(res)\n",
    "print(index)"
   ]
  }
