   "source": [
    "\"\"\"topk\n",
    "1.在一维中使用topk\n",
    "\"\"\"\n",
    "import torch as t\n",
    "x = t.arange(1,5)\n",
    "y = t.topk(x,3) # 返回的是一个元组\n",
    "print(x)\n",
    "top_index = y[1]  # type tensor\n",
    "print(top_index)"
   ]
