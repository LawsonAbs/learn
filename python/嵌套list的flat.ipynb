    "def flat(tree):\n",
    "  res = []\n",
    "  for i in tree:\n",
    "    if isinstance(i, list):\n",
    "      res.extend(flat(i)) # flat()返回的结果是一个list\n",
    "    else:\n",
    "      res.append(i)\n",
    "  return res\n",
    "\n",
    "out = flat(a)\n",
    "print(out)"
   ]
